package rules

import ma.medtech.drools.model.Person;

rule "Alerte senior"
agenda-group "senior"
when
  $p : Person(age >= 65)
then
  System.out.println("Senior detected : " + $p.getName());
end

rule "Alerte Junior"
agenda-group "junior"
when
  $p : Person(age < 18)
then
  System.out.println("Junior detected : " + $p.getName());
end
